{
  "name": "fatec-franca-api",
  "version": "1.0.0",
  "description": "Api do trabalho de TG",
  "main": "dist/index.js",
  "scripts": {
    "build": "scripts/build.sh",
    "postinstall": "scripts/build.sh",
    "start": "scripts/server.sh",
    "nodemon": "nodemon ./src/index.ts --watch",
    "nodemon:debug": "nodemon -e ts,json --exec node --inspect-brk -r ts-node/register src/index.ts",
    "test": "scripts/test.sh",
    "test:debug": "scripts/test-debug.sh",
    "migrate": "scripts/migrate.sh",
    "seed": "scripts/seed.sh",
    "lint": "tslint --project .",
    "lint:fix": "tslint --project . --fix",
    "lint:test": "tslint '__tests__/**/*.ts?(x)' ",
    "lint:test:fix": "tslint '__tests__/**/*.ts?(x)' --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juninmd/fatec-franca-api.git"
  },
  "keywords": [
    "fatec",
    "api",
    "franca"
  ],
  "author": "Antonio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/juninmd/fatec-franca-api/issues"
  },
  "homepage": "https://github.com/juninmd/fatec-franca-api#readme",
  "dependencies": {
    "@hapi/boom": "^7.4.3",
    "@hapi/hapi": "^18.3.2",
    "@hapi/joi": "^14.3.1",
    "axios": "^0.19.0",
    "dnscache": "^1.0.2",
    "dotenv": "^8.1.0",
    "fatec-franca-core-api": "^0.0.4",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^1.6.5",
    "rukia-core-hapi": "^0.0.10",
    "sequelize": "^5.16.0",
    "sequelize-cli": "^5.5.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.3.3",
    "@types/hapi__boom": "^7.4.1",
    "@types/hapi__hapi": "^18.2.5",
    "@types/hapi__joi": "^15.0.3",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.6.9",
    "@types/sequelize": "^4.28.4",
    "@types/validator": "^10.11.3",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "nock": "^10.0.6",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.3"
  }
}
